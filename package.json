{
   "name": "ios-timepicker-react",
   "version": "0.2.7",
   "description": "A modern time picker for your React app.",
   "main": "dist/TimePicker.js",
   "module": "dist/TimePicker.js",
   "typings": "dist/TimePicker.d.ts",
   "license": "MIT",
   "repository": {
      "type": "git",
      "url": "git+github.com:esamani77/react-ios-time-picker.git"
   },
   "files": [
      "dist",
      "README.md"
   ],
   "dependencies": {
      "react-portal": "^4.2.2"
   },
   "scripts": {
      "build": "webpack --mode production",
      "dev": "webpack --mode development --watch",
      "clean": "rm -rf dist",
      "prepare": "npm run build",
      "eslint": "eslint src --ext .js,.jsx",
      "eslint:fix": "npm run eslint -- --fix",
      "format": "prettier --write \"src/**/*.{js,jsx}\"",
      "test": "echo \"No tests specified\" && exit 0",
      "start-example": "cd example && npm start",
      "install-example": "cd example && npm install",
      "link-local": "npm link && cd example && npm link ios-timepicker-react"
   },
   "peerDependencies": {
      "react": "^18.2.0",
      "react-dom": "^18.2.0"
   },
   "eslintConfig": {
      "extends": [
         "react-app",
         "react-app/jest"
      ]
   },
   "browserslist": {
      "production": [
         ">0.2%",
         "not dead",
         "not op_mini all"
      ],
      "development": [
         "last 1 chrome version",
         "last 1 firefox version",
         "last 1 safari version"
      ]
   },
   "devDependencies": {
      "@babel/cli": "^7.18.10",
      "@babel/core": "^7.18.10",
      "@babel/preset-env": "^7.18.10",
      "@babel/preset-react": "^7.18.6",
      "babel-eslint": "^10.1.0",
      "babel-loader": "^8.2.5",
      "css-loader": "^6.7.1",
      "eslint": "^8.22.0",
      "eslint-config-airbnb": "^19.0.4",
      "eslint-config-prettier": "^8.5.0",
      "eslint-plugin-import": "^2.26.0",
      "eslint-plugin-jsx-a11y": "^6.6.1",
      "eslint-plugin-react": "^7.30.1",
      "eslint-plugin-react-hooks": "^4.6.0",
      "prettier": "^2.7.1",
      "react": "^18.2.0",
      "react-dom": "^18.2.0",
      "style-loader": "^3.3.1",
      "webpack": "^5.74.0",
      "webpack-cli": "^4.10.0"
   },
   "keywords": [
      "react time picker",
      "time picker",
      "ios time picker",
      "time picker react ios",
      "wheel time picker",
      "react ios time picker",
      "wheel picker",
      "select time"
   ],
   "author": "MEddarhri <mokhtareddarhri@gmail.com>",
   "directories": {
      "example": "example"
   },
   "bugs": {
      "url": "https://github.com/esamani77/react-ios-time-picker/issues"
   },
   "homepage": "https://github.com/esamani77/react-ios-time-picker#readme"
}
